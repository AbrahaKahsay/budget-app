<%= form_with(model: @category, local:true) do |f| %>
<div class="container">
  <div class="form-group">
    <%= f.label :name %><br />
    <%= f.text_field :name, class:"form-control",placeholder: "Please Enter a Category Name" %>
  </div>

  <div class="form-group">
    <%= f.label :icon %><br />
    <%= f.text_field :icon, class:"form-control",placeholder: "Please Enter an Icon" %>
  </div>

  <div class="submit-button">
    <%= f.submit class: "btn btn-outline-dark btn-lg" %>
  </div>
</div>
<% end %>